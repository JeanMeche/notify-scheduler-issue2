            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/robots.txt",
              "src/manifest.webmanifest",
              "src/firebase-messaging-sw.js",
              "src/sw-master.js"
            ],
            "styles": [
            ],
            "serviceWorker": "configs/ngsw-config.json",
