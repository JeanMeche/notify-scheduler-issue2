            },
            "fake_production": {
              "buildTarget": "acro-companion:build:production"
            },
            "old-production": {
              "browserTarget": "acro-companion:build:old-production"
            },
            "production": {
              "buildTarget": "acro-companion:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "acro-companion:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "no-cache": true,
            "configPath": "./jest.config.js",
            "reporters": ["jest-junit"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
